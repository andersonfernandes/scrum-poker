<div class="row">
  <% cards.each do |card|  %>
    <div class="col-lg-2 col-md-3 col-xs-4">
      <div class="card-container">
        <div class="card selectable">
          <div class="inner">
            <span class="card-label"><%= card.title  %></span>
          </div>
        </div>
      </div>
    </div>
  <% end  %>
</div>
